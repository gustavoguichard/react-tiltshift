{
  "extends": "eslint-config-airbnb",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "id-length": [2, {"properties": "never", "exceptions": ["_"]}],
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2,
    "semi": 0,
    "block-scoped-var": 0,
    "padded-blocks": 0
  },
  "plugins": [
    "react"
  ]
}